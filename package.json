    "bcrypt": "^6.0.0",
    "cloudinary": "^2.8.0",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^2.2.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "prisma": "^6.19.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^29.6.4",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "jest": {
    "testEnvironment": "node",
